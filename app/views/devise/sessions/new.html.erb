<div class="row">
  <div class="col m6 offset-m3 center">
    <h5>Log In</h5>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <%= render 'layouts/devisemes' %>
      <div class="input-field">
        <%= f.email_field :email, :id => "email", :class => "validate", :placeholder => "Email" %>
      </div>
      <div class="input-field">
        <%= f.password_field :password, :id => "email", :class => "validate", :placeholder => "Password", autocomplete: "off" %>
      </div>
      <button class="btn waves-effect waves-light" type="submit" name="action">Login</button>

    <% end %>
  </div>
</div>
